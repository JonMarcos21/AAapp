<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondobr"
    tools:context=".PlanesyPro">


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="85dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="109dp"
        app:srcCompat="@drawable/ataulfologo" />

    <ImageView
        android:id="@+id/bilingue"
        android:layout_width="171dp"
        android:layout_height="151dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="178dp"
        android:layout_marginEnd="127dp"
        android:contentDescription="erasmus"
        android:onClick="urlbilingue"
        app:srcCompat="@drawable/bilingue" />

    <ImageView
        android:id="@+id/erasmus"
        android:layout_width="192dp"
        android:layout_height="157dp"
        android:layout_below="@+id/bilingue"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="erasmus"
        android:onClick="urlerasmus"
        app:srcCompat="@drawable/erasmus" />

    <Button
        android:id="@+id/atras"
        android:layout_width="114dp"
        android:layout_height="58dp"
        android:layout_below="@+id/erasmus"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="274dp"
        android:background="@color/white"
        android:onClick="atras"
        android:text="@string/atras"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold" />
</RelativeLayout>